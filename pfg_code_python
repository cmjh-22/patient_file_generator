name=input("insert first name:")
lastname=input("insert last name:")
year=input("insert year the patient was attended the first time:")
dateofbirth=input("insert date ddmmyy:")
sex=input("M or F:")
patient_file= (name[0:2]+lastname[0:2]+"-"+year[0:]+"-"+ dateofbirth[0:]+sex[0])

print(patient_file)

